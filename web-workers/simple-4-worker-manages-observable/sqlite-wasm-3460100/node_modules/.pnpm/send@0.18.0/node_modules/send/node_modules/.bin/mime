#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bro/codes/wasm/web-workers-observable-wasm-sql/web-workers/simple-4-worker-manages-observable/sqlite-wasm-3460100/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/Users/bro/codes/wasm/web-workers-observable-wasm-sql/web-workers/simple-4-worker-manages-observable/sqlite-wasm-3460100/node_modules/.pnpm/mime@1.6.0/node_modules:/Users/bro/codes/wasm/web-workers-observable-wasm-sql/web-workers/simple-4-worker-manages-observable/sqlite-wasm-3460100/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bro/codes/wasm/web-workers-observable-wasm-sql/web-workers/simple-4-worker-manages-observable/sqlite-wasm-3460100/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/Users/bro/codes/wasm/web-workers-observable-wasm-sql/web-workers/simple-4-worker-manages-observable/sqlite-wasm-3460100/node_modules/.pnpm/mime@1.6.0/node_modules:/Users/bro/codes/wasm/web-workers-observable-wasm-sql/web-workers/simple-4-worker-manages-observable/sqlite-wasm-3460100/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
fi
